{
  "name": "azure-devops-testplans-api",
  "version": "1.0.0",
  "description": "A TypeScript project to interact with Azure DevOps Test Plans API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/server.js",
    "server": "ts-node src/server.ts",
    "server:build": "tsc && node dist/src/server.js",
    "client-example": "ts-node api-client-example.ts",
    "dev": "ts-node src/index.ts",
    "example": "ts-node example.ts",
    "example:build": "tsc && node dist/example.js",
    "clean": "rimraf dist"
  },
  "keywords": [
    "azure-devops",
    "test-plans",
    "api",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@azure/cosmos": "^4.5.0",
    "@azure/identity": "^4.0.1",
    "@octokit/rest": "^22.0.0",
    "azure-devops-node-api": "^12.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "helmet": "^7.2.0",
    "morgan": "^1.10.1",
    "openai": "^5.12.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.9",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
